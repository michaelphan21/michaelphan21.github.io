html, body {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}

#canvas, .logo, .nb, .page_divider {
	border: 1px solid black;	
	margin: 150px 200px 150px 200px;
  text-align: center;	
}

.logo, .nb, .content .page_divider {
	margin: 50px 200px 50px 200px;
}

.content {
	border: 1px solid black;	
  text-align: center;	
}

.page_divider {
	height: 30px; 
}

#header_list {
	font-size: 40px; 
	background-color: #FFFFFF; 
	padding: 0 10px;
}

button {
	background:none;
	border:none;
	outline-style:none;
}

a:hover, a:visited, a:link, a:active {
	color: black;
	text-decoration: none;
} 

.active {
	color: maroon;	
}




