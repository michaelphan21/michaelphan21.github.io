@font-face {
  font-family: myFirstFont;
  src: url(fonts/aqua.ttf);
}

html, body {
  width: 100%;
  height:100%;
	margin: 0; 
	padding: 0;   
}

.navbar{
	display:none;
}

.content, .logo, .page_divider {
	font-family: myFirstFont;
	margin: 50px 225px 50px 225px;
 	text-align: center;			
}

.page_divider {
	border-bottom: 2px solid black;	
	height: 15px;
	margin: 0 20% 0 20%; 
}

#header_list {
	font-size: 25px; 
	background-color: #FFFFFF; 
}

button {
	background:none;
	border:none;
	outline-style:none;
}

.active {
	color: red;	
}

.mySlides {
	width:100%;
	max-width: 750px;
	max-height: 500px;
	display:none;
}	

#fullname {
	font-family: myFirstFont;
 	text-align: center;	
	font-size: 60px;
}
#fullname a {
  text-decoration:none;
  color: inherit;	
}

#firstname {
	color: black;
}

#lastname {
	color: red;
}

#left_arrow button {
	margin: auto 0 0 5%;
}

#right_arrow button {
	margin: auto 5% 0 0;
}

p {
	font-family: sans-serif;
	font-size: 15px;
}

.topic {
	text-align: left;
	font-size: 35px;
	font-family: normal;	
}

.footer {
	width: 100%;
	margin:50px 0 50px 0;
	text-align:center;
}

.row{
	text-align:left;
}

.row.contact{
	text-align:center;	
}

.col-xs-12.col-sm-12.col-md-4.col-xl-4 {
	height:450px;
	border: 1px solid black;
	text-align:center;
}

/* Extra small devices (portrait phones, less than 576px) */
@media (max-width: 575.98px) {
	.logo, .page_divider{
		display:none;	
	}

	.navbar {
		font-family: myFirstFont;
		display:block;
	}
	
	.container, .navbar-header > button {
		margin: auto;
		width: 100%;
		padding: 0 0 0 0;
		text-align:center;
	}
	
	.container {
		background-color: white;	
	}
	
	.toggle_divider {
		border-bottom: 1px solid black;
		margin: 0 47% 0 47%;		
	}
	
	.content {
		margin: 70px auto;
		padding: auto;
		width: 95%;
		text-align:center;
	}
	
	.canvas .page_divider {
		display:none;
	}

	#header_list {
		display:none;
	}

	#fullname {
		margin: 15px 0 0 0;	
		font-size: 25px;
	}

	#left_arrow button {
		margin: auto 0 0 auto;
	}

	#right_arrow button {
		margin: auto auto 0 0;
	}

	p {
		font-family: sans-serif;
		font-size: 15px;
		text-align: center;		
	}
	
	.topic {
		text-align: center;
		font-family: normal;
		font-size: 22px;	
	}
	
	.outline {
		display:inline:
	}
}

