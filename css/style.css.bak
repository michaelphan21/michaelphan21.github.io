@font-face {
  font-family: myFirstFont;
  src: url(fonts/Aqua.ttf);
}

#canvas, .logo, .nb, .page_divider {
	font-family: myFirstFont;
}

html, body {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}

#canvas {
	border: 1px solid black;	
}

#canvas, .logo, .nb, .content, .page_divider {
	margin: 50px 200px 50px 200px;
  text-align: center;		
}

.page_divider {
	border-bottom: 2px solid black;
	height: 15px; 
}

#header_list {
	font-size: 25px; 
	background-color: #FFFFFF; 
	padding: 0 10px;
}

button {
	background:none;
	border:none;
	outline-style:none;
}

.active {
	color: red;	
}
	
.mySlides {
	width:100%;
	display:none;
}	

#index {
	font-size: 60px;
}
#index a{
  text-decoration:none;
  color: inherit;	
}

#firstname {
	color: black;
}

#lastname {
	color: red;
}

#left_arrow button {
	margin: 50px 0 0 400px;
}

#right_arrow button {
	margin: 50px 400px 0 0;
}
.w3-button w3-black w3-display-right{
	
}

p {
	font-family: sans-serif;
	font-size: 15px;
}

#intro {
	margin: 50px 200px 50px 200px;
  text-align: center;		
}
